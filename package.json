{
  "name": "nuxt-graphql-codegen",
  "description": "Nuxt module for GraphQL Code Generator",
  "version": "0.1.3",
  "homepage": "https://github.com/lewebsimple/nuxt-graphql-codegen#readme",
  "bugs": {
    "url": "https://github.com/lewebsimple/nuxt-graphql-codegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewebsimple/nuxt-graphql-codegen.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "release-it"
  },
  "build": {
    "externals": [
      "chokidar",
      "consola",
      "pathe"
    ]
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^2.4.5",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "graphql": "^16.3.0",
    "nuxt3": "latest",
    "release-it": "^14.12.5"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "before:init": "yarn prepack"
    },
    "github": {
      "release": true
    }
  }
}
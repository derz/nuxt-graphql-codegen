{
  "name": "nuxt-graphql-codegen",
  "description": "Nuxt module for GraphQL Code Generator",
  "version": "0.1.5",
  "homepage": "https://github.com/lewebsimple/nuxt-graphql-codegen#readme",
  "bugs": {
    "url": "https://github.com/lewebsimple/nuxt-graphql-codegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewebsimple/nuxt-graphql-codegen.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "release": "release-it"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest"
  },
  "devDependencies": {
    "@lewebsimple/eslint-config-vue": "^0.5.2",
    "@nuxt/module-builder": "latest",
    "eslint": "^8.10.0",
    "graphql": "^16.3.0",
    "nuxt3": "latest",
    "release-it": "^14.12.5"
  },
  "peerDependencies": {
    "graphql": "^15 || ^16"
  },
  "unbuild": {
    "externals": [
      "chokidar",
      "consola",
      "graphql",
      "pathe"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "hooks": {
      "before:init": "yarn build"
    },
    "github": {
      "release": true
    }
  },
  "eslintConfig": {
    "extends": "@lewebsimple/eslint-config-vue"
  }
}